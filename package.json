{
  "name": "document2scss",
  "displayName": "document2scss",
  "description": "a util can extract class to scss  from selected document!",
  "version": "0.0.1",
  "keywords": [
    "html",
    "css",
    "scss",
    "html2css",
    "html2scss"
  ],
  "icon": "images/icon.png",
  "publisher": "moqi",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "document2scss.document2scss",
        "title": "document2scss",
        "category": "document2scss"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "document2scss.document2scss",
          "alt": "document2scss.document2scss",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "when": "editorHasSelection",
          "command": "document2scss.document2scss",
          "alt": "document2scss.document2scss",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/js-beautify": "^1.13.1",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/parse5": "^6.0.0",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "js-beautify": "^1.13.13",
    "parse5": "^6.0.1"
  }
}
